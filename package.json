{
  "name": "rtunes",
  "version": "1.2.9",
  "description": "A React app to play music from reddit. Based on Pete Cook <pete@cookpete.com> (http://github.com/cookpete)",
  "author": "Jake",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/jake-g/rtunes"
  },
  "scripts": {
    "clean": "rimraf dist data",
    "start": "node prodServer.js",
    "dev": "node devServer.js",
    "fetch-playlists": "babel-node src/utils/fetch-playlists.js",
    "prebuild": "npm run clean && npm run fetch-playlists",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "postbuild": "cp index.html dist && cp images/favicon.ico dist",
    "deploy": "npm run build && git commit dist/ data/ -m 'new build' && git push heroku -u master"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.0",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.11",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "express": "^4.14.0",
    "insert-css": "^2.0.0",
    "lscache": "^1.0.5",
    "normalize.css": "^3.0.3",
    "query-string": "^3.0.0",
    "randomcolor": "^0.4.4",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-player": "^0.5.8",
    "react-router": "^2.0.0-rc5",
    "react-timeago": "^2.2.1"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
