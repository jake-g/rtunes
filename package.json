{
  "name": "rtunes",
  "version": "1.4.0",
  "description": "A React app to play music from reddit.",
  "author": "Jake",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/jake-g/rtunes"
  },
  "scripts": {
    "clean": "rimraf dist data",
    "start": "node prodServer.js",
    "dev": "node devServer.js",
    "fetch-playlists": "babel-node src/utils/fetch-playlists.js",
    "prebuild": "npm run clean && npm run fetch-playlists",
    "build": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "postbuild": "cp index.html dist && cp images/favicon.ico dist && cp dist/* mobile/",
    "desktop": "node ./desktop/build-apps.js",
    "start-native": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "cd mobile/ && react-native run-ios",
    "android": "cd mobile/ && react-native run-android",
    "android-release": "cd mobile/android/ && ./gradlew assembleRelease && cd .. && react-native run-android --variant=release",
    "deploy": "npm run build && git commit dist/ data/ mobile/ -m 'new build' && git push heroku -u master"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.28.1",
    "electron-installer-windows": "^0.2.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "node-libs-browser": "^2.0.0",
    "node-sass": "^4.5.2",
    "rimraf": "^2.5.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "express": "^4.14.0",
    "insert-css": "^2.0.0",
    "lscache": "^1.0.5",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.8",
    "query-string": "^4.3.4",
    "randomcolor": "^0.5.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-player": "^0.17.0",
    "react-router": "^2.8.1",
    "react-timeago": "3.1.2"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
